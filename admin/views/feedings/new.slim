ul class="nav nav-tabs"
  li = link_to tag_icon(:list, pat(:list)), url(:feedings, :index)
  li class="active" = link_to tag_icon(:plus, pat(:new)), url(:feedings, :new)

div class="tabs-content"
  - form_for :feeding, url(:feedings, :create), :class => 'form-horizontal' do |f|
    = partial 'feedings/form', :locals => { :f => f }
